<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #BAE3E0;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.teamc.controllers.FloorPlanController">
   <children>
      <AnchorPane layoutX="12.0" layoutY="13.0" prefHeight="373.0" prefWidth="575.0" style="-fx-background-color: #008080;">
         <children>
            <MenuButton fx:id="menuButton" layoutX="84.0" layoutY="14.0" mnemonicParsing="false" onAction="#getMenuButton" prefHeight="25.0" prefWidth="407.0" style="-fx-background-color: #ffffff;" text="--Select Floor Level--" textFill="TEAL">
               <items>
                  <MenuItem fx:id="choice1" mnemonicParsing="false" onAction="#getChoice1" text="Floor 1" />
                  <MenuItem fx:id="choice2" mnemonicParsing="false" onAction="#getChoice2" text="Floor 2" />
                  <MenuItem fx:id="choice3" mnemonicParsing="false" onAction="#getChoice3" text="Floor 3" />
                  <MenuItem fx:id="choice4" mnemonicParsing="false" onAction="#getChoice4" text="Ian's Conference Room" />
               </items>
               <font>
                  <Font name="Arial Bold" size="12.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </MenuButton>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
